{
  "name": "pos-css-starter",
  "version": "0.1.0",
  "description": "Another post css starter kit",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf ./public postcss ./src/input.css --use autoprefixer --output ./public/pos-css-starter.css --watch",
    "build": "rm -rf ./dist && postcss ./src/input.css --use autoprefixer --output ./dist/pos-css-starter.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "cssnano": "^5.0.2",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "postcss-preset-env": "^6.7.0"
  }
}
