{
  "name": "pos-css-starter",
  "version": "0.1.0",
  "description": "Another post css starter kit",
  "main": "index.js",
  "scripts": {
    "start": "postcss ./src/index.css --use postcss-preset-env postcss-import postcss-extend autoprefixer --output ./public/pos-css-starter.css --watch",
    "build": "postcss ./src/index.css --use postcss-preset-env postcss-import postcss-extend autoprefixer --output ./dist/pos-css-starter.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "cssnano": "^5.0.2",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.0.1",
    "postcss-preset-env": "^6.7.0"
  }
}
